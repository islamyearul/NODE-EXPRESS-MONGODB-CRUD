
 <% users.forEach(function(user, i){ %>
    <tr>
        <th scope="row"> <%= i+1 %> </th>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.gender %></td>
        <td><%= user.status %></td>
        <td>
            <!-- <a href="#" class="btn btn-primary btn-sm"><i class="fas fa-eye"></i></a> -->
            <a href="/update-user?id=<%= user._id %>" class="btn btn-success btn-sm"><i class="fas fa-edit"></i></a>
            <a class="btn btn-danger btn-sm delete_user" data-id="<%= user._id %>"><i class="fas fa-trash"></i></a>

        </td>
    </tr>

  
  <% }); %>



